{
  "name": "Background Message",
  "version": "0.01",
  "description": "Listen in tie background",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
